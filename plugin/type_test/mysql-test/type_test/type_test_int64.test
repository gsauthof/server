--vertical_results
SELECT
  PLUGIN_NAME,
  PLUGIN_VERSION,
  PLUGIN_STATUS,
  PLUGIN_TYPE,
  PLUGIN_AUTHOR,
  PLUGIN_DESCRIPTION,
  PLUGIN_LICENSE,
  PLUGIN_MATURITY,
  PLUGIN_AUTH_VERSION
FROM INFORMATION_SCHEMA.PLUGINS
  WHERE PLUGIN_TYPE='DATA TYPE'
    AND PLUGIN_NAME='TEST_INT64';
--horizontal_results


CREATE TABLE t1 (a TEST_INT64);
SHOW CREATE TABLE t1;
DROP TABLE t1;

SELECT CAST('100' AS TEST_INT64) AS cast;

DELIMITER $$;
BEGIN NOT ATOMIC
  DECLARE a TEST_INT64 DEFAULT 256;
  SELECT HEX(a), a;
END;
$$
DELIMITER ;$$

CREATE FUNCTION f1(p TEST_INT64) RETURNS TEST_INT64 RETURN 1;
SHOW CREATE FUNCTION f1;
SELECT f1(10);
DROP FUNCTION f1;
